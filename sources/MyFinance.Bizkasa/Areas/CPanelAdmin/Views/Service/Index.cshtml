@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/CPanelAdmin/Views/Shared/_LayoutAdmin.cshtml";
}

<script src="~/Areas/CPanelAdmin/Script/Service.js"></script>

<div class="main-content">
    <div ng-view="" class="ng-scope">
        <div data-ng-controller="productController" class="ng-scope">
            <div class="breadcrumbs breadcrumbs-fixed" id="breadcrumbs">
                <h3>Danh sách </h3>
                <div class="toolbar">
                    <a class="btn btn-primary" href="#userhotel">
                        <i class="icon-plus"></i>
                        <span class="hidden-320">Thêm dịch vụ</span>
                    </a>
                    <a class="btn btn-danger" >
                        <i class="menu-icon fa fa-bolt"></i>
                        <span class="hidden-320">Xóa nhiều</span>
                    </a>
                    <div class="btn-group">
                        <button data-toggle="dropdown" class="btn btn-primary dropdown-toggle">
                            <i class="icon-file"></i>
                            <span class="hidden-320">Excel</span>
                            <i class="icon-caret-down icon-on-right"></i>
                        </button>
                        <ul class="dropdown-menu">
                            <li>
                                <a data-ng-click="openPopupImport()">
                                    <i class="icon-upload-alt"></i><span class="hidden-320">Nhập Excel</span>
                                </a>
                            </li>
                            <li>
                                <a data-ng-click="exportExcel()">
                                    <i class="icon-download-alt"></i><span class="hidden-320">Xuất Excel</span>
                                </a>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
            <div class="page-content">
                <div class="page-content-container">
                    <div class="row">
                        <div class="col-xs-12">
   
                            <div class="col-xs-12">
                                <div class="col-xs-12 no-padding">
                                    <div class="form-group" style="overflow: hidden;">
                                        <div class="col-sm-12 no-padding-left">
                                            <div class="col-sm-6 no-padding-left">
                                                <input type="text" id="filter" name="txtSearch" value="" placeholder=" Nhập tên, mă hoặc SĐT khách hàng" class="col-xs-12 search-query ng-valid ng-dirty">
                                            </div>
                                            <div class="col-sm-6 no-padding-left no-padding-right">
                                                <div class="input-group" style="width:100%">
                                                    <span>
                                                        @*@Html.DropDownList("CatalogID", (IEnumerable<SelectListItem>)ViewBag.CatalogID, "=== Chọn SP ===", new { @class = "form-control ng-valid ng-dirty", @style = "margin-left: 5px; width: 40%;", @onchange = "return _filterRoles(this)" })*@
                                                    </span>
                                                    <span class="padding-left-10">
                                                        <button id="btnSearch" class="btn btn-primary " onclick="return _filter()">
                                                            <i class="icon-search icon-on-right bigger-110"></i>
                                                            <span class="hidden-320">Tìm kiếm</span>
                                                        </button>
                                                    </span>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>


                            </div>
                            <div class="dataTables_wrapper" role="grid">
                                <table class="table table-hover dataTable ng-scope ng-table" ng-table="tableParams" id="list">
                                    <thead>
                                        <tr role="row">
                                            <th class="width-27px border hidden-480"></th>
                                            <th class="sorting border">Tiêu đề</th>
                                            <th class="center border hidden-480">Danh mục</th>
                                            <th class="center border white-bg">Link</th>
                                            <th class="center width-5 hidden-480">Hình</th>
                                            <th class="center border hidden-480">Status</th>
                                            <th class="hidden-480 text-center white-bg">Thao tác</th>

                                        </tr>
                                    </thead>

                                    <tbody style="border-bottom: none;" class="ng-scope">
                                      

                                            <tr style="cursor: pointer;" class="oddrow">
                                                <td class="hidden-480">
                                                    <label class="no-padding-left">
                                                        <input type="checkbox" class="ace ng-pristine ng-valid" ><span class="lbl"></span>
                                                    </label>
                                                </td>
                                                <td sortable="'Productname'">
                                                  
                                                  
                                                    <!-- ngIf: p.productItems.length > 1 -->
                                                </td>

                                                <td class="width-10 hidden-480 ng-binding">
                                                    
                                                </td>
                                                <td class="width-10 hidden-480 ng-binding">
                                                    <a class="ng-binding" target="_blank" href=""></a>
                                                </td>
                                                <td class="center width-5 hidden-480">
                                                    <img class="product-thumb" title="Xem hình" src="">
                                                </td>
                                                <td class="center width-5 hidden-480">
                                                    

                                                </td>
                                                <td class="center width-7">
                                                    <div class="visible-md visible-lg hidden-sm hidden-xs">
                                                        <a>
                                                            <i class="menu-icon fa fa-pencil-square-o bigger-130"> Sửa</i>
                                                        </a>||

                                                        <a onclick="">
                                                            <i class="icon-trash bigger-130" title="Xóa hẳn hàng hóa"> Xóa</i>
                                                        </a>
                                                    </div>
                                                </td>
                                            </tr>
                                        <!-- ngIf: p.productItems.length > 1 -->
                                    </tbody><!-- end ngRepeat: p in params.data -->
                                </table>
                            
                            </div>
                            <!-- The Gallery as lightbox dialog, should be a child element of the document body -->
                            <div id="blueimp-gallery" class="blueimp-gallery blueimp-gallery-controls" data-use-bootstrap-modal="false">
                                <!-- The container for the modal slides -->
                                <div class="slides"></div>
                                <!-- Controls for the borderless lightbox -->
                                <h3 class="title"></h3>
                                <a class="prev">‹</a>
                                <a class="next">›</a>
                                <a class="close">×</a>
                                <a class="play-pause"></a>
                                <ol class="indicator"></ol>
                                <!-- The modal dialog, which will be used to wrap the lightbox content -->
                                <div class="modal fade">
                                    <div class="modal-dialog">
                                        <div class="modal-content">
                                            <div class="modal-header">
                                                <button type="button" class="close" aria-hidden="true">×</button>
                                                <h4 class="modal-title"></h4>
                                            </div>
                                            <div class="modal-body next"></div>
                                            <div class="modal-footer">
                                                <button type="button" class="btn btn-default pull-left prev">
                                                    <i class="glyphicon glyphicon-chevron-left"></i>
                                                    Previous
                                                </button>
                                                <button type="button" class="btn btn-primary next">
                                                    Next
                                                    <i class="glyphicon glyphicon-chevron-right"></i>
                                                </button>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        
                        </div>
                    </div>
                </div>
                <div class="footer">
                    <div class="pull-left">
                        <span>Tổng số sản phẩm: </span><span class="total-highlight ng-binding"></span>

                    </div>
                 
                </div>
            </div>
          
        </div>


    </div>



</div>


<div id="userhotel" class="modal fade bs-example-modal-lg" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">

            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                <h4 class="modal-title" id="myLargeModalLabel">Large modal</h4>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-md-12">
                        <div class="form-group">
                            <input type="text" class="form-control" placeholder="To" required="">
                        </div>

                    </div>
                </div>
                <div class="widget-body">
                    <table class="table table-striped table-bordered table-hover">
                        <thead>
                            <tr>
                                <th class="text-center">
                                    <div class="checker"><span class=""><input type="checkbox" class="group-checkable" data-set="#flip .checkboxes"></span></div>
                                </th>
                                <th>
                                    UserName
                                </th>
                                <th>
                                    Họ và tên
                                </th>


                                <th class="text-center">
                                    Email
                                </th>
                                <th>
                                    Action
                                </th>
                            </tr>
                        </thead>
                        <tbody >

                            <tr>
                                <td>
                                    <div class="checker" style="min-width:15px;"><span class=""><input type="checkbox" class="checkboxes" name="Id_group" value="" ng-model="us.isChecked" ng-change="Selected()"></span></div>
                                </td>
                                <td>
                                   
                                <td>
                                   
                                </td>


                                <td>
                                   
                                </td>
                                <td class="center ">
                                    <a id="bootbox-options" href="javasctipt:">AddUser</a>
                                </td>
                            </tr>

                        </tbody>
                    </table>
                </div>
                <div class="btn-group">

                    <button type="button" class="btn btn-blue active" ng-click="SaveUserHotel()">Chọn</button>
                </div>


            </div>
        </div><!-- /.modal-content -->
    </div><!-- /.modal-dialog -->
</div>