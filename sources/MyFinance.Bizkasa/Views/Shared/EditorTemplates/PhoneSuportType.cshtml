@using FailTracker.Web.Domain
@model FailTracker.Web.Domain.PhoneSuportType
@{
    var options = Enum.GetValues(typeof(PhoneSuportType))
    .Cast<PhoneSuportType>()
    .Select(t => new SelectListItem
    {
        Text = t.ToString(),
        Value = t.ToString(),
        Selected = t == Model
    })
    .ToArray();

}
@Html.DropDownList("", options)


